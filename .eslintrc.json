{
  "root": true,
  "parserOptions": {
    "project": true,
    "tsconfigRootDir": "."
  },
  "ignorePatterns": ["/vite.config.ts", "/dist"],
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base"
  ],
  "rules": {
    "no-console": "off",
    "import/prefer-default-export": "off",
    "arrow-body-style": "off"
  }
}
